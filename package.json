{
  "name": "libera_2.0",
  "version": "1.0.0",
  "scripts": {
    "local": "npx sls offline start --local",
    "start": "node --max-old-space-size=5120 node_modules/serverless/bin/serverless.js offline start",
    "start:preSignUp": "sls invoke local -f preSignUp -p ./resources/mocks/pre-signup.json",
    "start:postConfirmation": "sls invoke local -f postConfirmation -p ./resources/mocks/post-confirm-sign-up.json",
    "start:preAuthentication": "sls invoke local -f preAuthentication -p ./resources/mocks/pre-authentication.json",
    "start:preGenerationTokenNewChangellePassword": "sls invoke local -f preTokenGeneration -p ./resources/mocks/pre-token-generation.json",
    "start:saveEnterpriseLinkRequest": "sls invoke local -f saveEnterpriseLinkRequest -p ./resources/mocks/save-enterprise-link-request.event.json",
    "start:sendEnterpriseLinkRequest": "sls invoke local -f sendEnterpriseLinkRequest -p ./resources/mocks/send-enterprise-link-request.event.json",
    "start:sendInvoiceBulk": "sls invoke local -f invoicesBulkLoad -p ./resources/mocks/invoices-bulk-load.event.json",
    "start:errorInvoiceBulkLoad": "sls invoke local -f errorInvoiceBulkLoad -p ./resources/mocks/error-invoce-bulk-load.event.json",
    "start:saveInvoiceNegotiationRecord": "sls invoke local -f saveInvoiceNegotiationRecord -p ./resources/mocks/save-negotiation-record.event.json",
    "start:saveInvoiceFundingRecord": "sls invoke local -f saveInvoiceFundingRecord -p ./resources/mocks/save-funding-record.event.json",
    "start:saveEnterpriseRecord": "sls invoke local -f saveEnterpriseRecord -p ./resources/mocks/save-enterprise-record.event.json",
    "sls": "serverless",
    "test:authorizer": "serverless invoke local -f Authorizer --aws-profile bocc -p event.json"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "lambda-proxy-utils": "^1.4.5",
    "lerna": "^3.17.0",
    "lodash": "latest",
    "middy": "^0.25.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.34",
    "mongodb": "^3.6.2",
    "mysql": "^2.16.0",
    "node-fetch": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "soap": "^0.43.0",
    "string": "^3.3.3",
    "tslib": "2.2.0",
    "typeorm": "^0.2.16",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.1",
    "@types/node": "^11.12.2",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.395.0",
    "cache-loader": "^3.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "gnomon": "^1.5.0",
    "happypack": "^5.0.1",
    "serverless": "^1.42.3",
    "serverless-deployment-bucket": "^1.1.2",
    "serverless-offline": "^4.9.3",
    "serverless-plugin-split-stacks": "1.5.4",
    "serverless-webpack": "^5.1.1",
    "terser-webpack-plugin": "^1.3.0",
    "thread-loader": "^2.1.2",
    "ts-loader": "^6.0.2",
    "typescript": "^3.2.2",
    "webpack": "^4.5.0",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
